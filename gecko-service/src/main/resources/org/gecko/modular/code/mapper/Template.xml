<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gecko.modular.code.mapper.TemplateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.gecko.modular.code.entity.Template">
        <id column="id" property="id" />
        <result column="project_id" property="projectId" />
        <result column="title" property="title" />
        <result column="template_type" property="templateType" />
        <result column="file_name" property="fileName" />
        <result column="file_path" property="filePath" />
        <result column="file_type" property="fileType" />
        <result column="content" property="content" />
        <result column="activated" property="activated" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_id, title, template_type, file_name, file_path, file_type, content, activated
    </sql>

</mapper>
