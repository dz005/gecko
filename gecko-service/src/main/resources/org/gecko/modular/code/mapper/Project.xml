<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gecko.modular.code.mapper.ProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.gecko.modular.code.entity.Project">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="title" property="title" />
        <result column="owner" property="owner" />
        <result column="icon_class" property="iconClass" />
        <result column="encoding" property="encoding" />
        <result column="description" property="description" />
        <result column="code_package" property="codePackage" />
        <result column="xml_path" property="xmlPath" />
        <result column="js_path" property="jsPath" />
        <result column="html_path" property="htmlPath" />
        <result column="db_count" property="dbCount" />
        <result column="table_count" property="tableCount" />
        <result column="template_count" property="templateCount" />
        <result column="member_count" property="memberCount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, title, owner, icon_class, encoding, description, code_package, xml_path, js_path, html_path, db_count, table_count, template_count, member_count
    </sql>

</mapper>
