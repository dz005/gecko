<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gecko.modular.code.mapper.DbMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.gecko.modular.code.entity.Db">
        <id column="id" property="id" />
        <result column="project_id" property="projectId" />
        <result column="title" property="title" />
        <result column="type" property="type" />
        <result column="url" property="url" />
        <result column="username" property="username" />
        <result column="password" property="password" />
        <result column="table_prefix" property="tablePrefix" />
        <result column="db_qualifier" property="dbQualifier" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_id, title, type, url, username, password, table_prefix, db_qualifier
    </sql>

</mapper>
